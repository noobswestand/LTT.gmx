}

with(par_block){
    if point_distance(x,y,other.x,other.y)&lt;50{
        helth-=(1/(point_distance(x,y,other.x,other.y)+1))*25
    }
}

with(par_monster){
    if point_distance(x,y,other.x,other.y)&lt;50{
        helth-=(1/(point_distance(x,y,other.x,other.y)+1))*25
    }
}
