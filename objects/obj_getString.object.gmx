draw_set_alpha(boxa2)
draw_set_color(c_ltgray)
draw_rectangle(xx-150-searchw,yy,xx+150+searchw,yy+45,false)
if searchGo=false{
    searchw+=(0-searchw)/3
    if scr_click4(xx-150-searchw,yy,xx+150+searchw,yy+45){
        searchGo=true
        searchType=true
    }
    draw_set_color(c_dkgray)
    draw_set_halign(fa_center)
    if searchType=false{
        draw_set_color(c_gray)
        draw_text_ext_transformed(xx,yy+7,"Type Something...",0,9999999,0.75,0.75,0)
    }else{
        draw_text_ext_transformed(xx,yy+7,string_replace_all(searchStr,"#","\#"),0,9999999,0.75,0.75,0)
    }
    
}else{
    draw_set_color(c_dkgray)
    if round(current_second/2)*2=current_second || searchW!=0{
        draw_set_alpha(boxa1)
        draw_rectangle(xx-145-searchw+searchx,yy+5,xx-140-searchw+searchW+searchx,yy+40,false)
    }
    draw_set_font(fnt_norm)
    draw_set_halign(fa_center)
    draw_text_ext_transformed(xx,yy+7,string_replace_all(searchStr,"#","\#"),0,9999999,0.75,0.75,0)
    searchStr+=keyboard_string
    keyboard_string=""
    
    if keyboard_check(vk_control) &amp;&amp; keyboard_check_pressed(ord("V")){
        searchStr+=clipboard_get_text()
    }
    if keyboard_check(vk_control) &amp;&amp; keyboard_check_pressed(ord("C")){
        clipboard_set_text(searchStr)
    }
    
    if searchW!=0{
        if keyboard_check_pressed(vk_anykey){
            searchStr=""
            searchW=0
            searchx0=0
        }
    }
    if keyboard_check(vk_control) &amp;&amp; keyboard_check_pressed(ord("A")){
        searchW=((xx-150-searchw)-(xx+135+searchw))*-1//(string_width(searchStr))*0.75
        searchx0=0
    }
    searchx+=(searchx0-searchx)/3
    
    
    
    if string_width(searchStr)/2&gt;500{
        
        /*
        repeat(((string_width(searchStr)/2)/250)+1){
            searchStr=string_delete(searchStr,string_length(searchStr),1)
        }
        */
        
    }
    if keyboard_check(vk_backspace){
        if searchB=0{
            searchStr=string_delete(searchStr,string_length(searchStr),1)
        }else{
            if searchB&gt;30{
                searchStr=string_delete(searchStr,string_length(searchStr),1)
            }
        }
        searchB+=1
    }else{searchB=0}
    
    if string_width(searchStr)&lt;300{
        searchw+=(25-searchw)/3
    }else{
        searchw+=((((string_width(searchStr)/2.75)-135)-searchw)/3)
    }
    if scr_click4(xx-150-searchw,yy,xx+150+searchw,yy+45)=false
    &amp;&amp; mouse_check_button_pressed(mb_left){
        if searchStr=""{
            searchGo=false
            searchType=false
        }else{
            searchGo=false
        }
    }
}


