        draw_sprite_ext(spr_lock,0,xxoff+((room_width/2)-1*Wperson[i,3])+Wperson[i,4]+4,yy-(1*Wperson[i,3])+Wperson[i,5],
        Wperson[i,3]*7,Wperson[i,3]*7,0,c_white,Wperson[i,6])
        }
    }
    
}




