            vspeed=-(jumpspeed+random_range(0,2))*scr_speedCalc()
        }
        if (place_meeting(x+1,y,par_block)) &amp;&amp; vspeed=0{
            vspeed=-(jumpspeed+random_range(0,2))*scr_speedCalc()
        }
        if (place_meeting(x-1,y,par_block)) &amp;&amp; vspeed=0{
            vspeed=-(jumpspeed+random_range(0,2))*scr_speedCalc()
        }
        
    }
    
    
    if playerx&gt;x+25{hspeed=1*scr_speedCalc()}
    if playerx&lt;x-25{hspeed=-1*scr_speedCalc()}
