                playery=obj_client2.y
            }
        }else{
            var playerx,playery;
            playerx=obj_player.x
            playery=obj_player.y
        }
    }
    
    
    
    
    if !instance_exists(obj_client){
    if helth&lt;=0{instance_destroy()}
    
    var jumpspeed=5
    if (place_meeting(x,y+1,par_block)){
