    &amp;&amp; image_index=1 &amp;&amp; sprite_index=spr_monster3 &amp;&amp; abs(image_speed)=0.05
    &amp;&amp; collision_line(x,y,x+lengthdir_x(50,dir),y+lengthdir_y(50,dir),par_block,0,0)&lt;=0{
